{
  "name": "pseudoqurl",
  "version": "__CAPUCHIN_VERSION__",
  "manifest_version": 3,
  "description": "PROMOTE THIS!",
  "browser_action": {
    "default_title": "PseudoQURL Extension",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://pseudoqurl.heroku.com/*",
    "http://localhost:8080/*",
    "webNavigation",
    "identity",
    "identity.email",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "background_bndl.js"
    ]
  }
}