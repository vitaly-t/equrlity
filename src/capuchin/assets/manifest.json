{
  "name": "__CAPUCHIN_NAME__",
  "version": "__CAPUCHIN_VERSION__",
  "manifest_version": 2,
  "description": "Bookmarks, Content Management and more.",
  "browser_action": {
    "default_title": "__CAPUCHIN_NAME__",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "identity",
    "identity.email",
    "http://*/*",
    "https://*/*"
  ],
  "auth_entries": "",
  "background": {
    "persistent": true,
    "scripts": [
      "background_bndl.js"
    ]
  },
  "icons": {
    "128": "images/__ICON_128__",
    "32": "images/__ICON_32__"
  }
}