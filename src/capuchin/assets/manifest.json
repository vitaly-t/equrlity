{
  "name": "pseudoqurl",
  "version": "__CAPUCHIN_VERSION__",
  "manifest_version": 2,
  "description": "Bookmarks, Content Management and more.",
  "browser_action": {
    "default_title": "PseudoQURL",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://pseudoqurl.heroku.com/*",
    "https://*.pseudoq.com/*",
    "https://pseudoq.com/*",
    "http://localhost:8080/*",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "identity",
    "identity.email",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*"
  ],
  "auth_entries": "",
  "background": {
    "persistent": true,
    "scripts": [
      "background_bndl.js"
    ]
  },
  "icons": {
    "128": "images/__ICON_128__",
    "32": "images/__ICON_32__"
  }
}