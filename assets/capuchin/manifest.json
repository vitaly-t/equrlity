{
  "name": "Synereo",
  "version": "0.6",
  "manifest_version": 3,
  "description": "AMPLIFY THIS!",
  "browser_action": {
    "default_title": "Synereo Extension",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "https://synereo-amplitude.heroku.com/*",
    "http://localhost:8080/*",
    "webNavigation"
  ],
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  }
}
